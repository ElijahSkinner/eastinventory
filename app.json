{
  "expo": {
    "name": "EAST Initiative Procurement",
    "slug": "east-inventory-app",
    "version": "1.0.0",
    "entryPoint": "./App.tsx",
    "orientation": "portrait",
    "icon": "./assets/logos/EAST_Logo_2c_vertical.png",
    "scheme": "eastinventoryapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0093B2",
        "foregroundImage": "./assets/logos/EAST_Logo_2c_vertical.png",
        "backgroundImage": "./assets/logos/EAST_Logo_2c_vertical.png",
        "monochromeImage": "./assets/logos/EAST_Logo_black_vertical.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
